{
  "name": "store-manager",
  "version": "1.0.0",
  "engines": {
    "node": "8.9.1"
  },
  "description": "Store Manager is a web application that helps store owners manage sales and product inventory records.",
  "main": "index.js",
  "scripts": {
    "start": "babel-node --presets env,stage-2 ./build/bin/www",
    "devstart": "nodemon --exec babel-node --presets env,stage-2 ./server/bin/www",
    "test": "nyc mocha --exit --timeout 10000 ./build/test/**/*.js --compilers js:babel-core/register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint",
    "build-babel": "babel --out-dir ./build --source-maps --copy-files ./server",
    "build": "mkdir build && npm run build-babel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mecsoccer/Store-Manager.git"
  },
  "author": "Jaachimma Onyenze",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mecsoccer/Store-Manager/issues"
  },
  "homepage": "https://github.com/mecsoccer/Store-Manager#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-validator": "^5.3.0",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.3.0",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.1.0"
  }
}
